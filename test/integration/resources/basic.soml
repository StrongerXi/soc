let min x y =
  if y < x
  then y
  else x
;;

let rec fac n =
  if (n < 0) || (n = 0)
  then 1
  else n * (fac (n - 1))
;;

print (fac 0)
;;
print (fac 10)
;;
print (33 * (0 - 3))
;;
print (min (0 - 7) 20)
;;
print (min 7 20)
;;
